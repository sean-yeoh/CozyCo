<h1>All Notices</h1>


<table class="table table-striped">
    <tr>
      <th>Title</th>
      <th>Notice</th>
      <th>Created At</th>


      <% @notices.each do |notice| %>

      <tr>
        <td><%= notice.title %></td>
        <td><%= notice.body %></td>
        <td><%= notice.created_at %></td>
      </tr>
      <% end %>
    </table>

<hr>
<%= link_to 'New Notice', new_notice_path(current_user), class: "btn btn-primary" %>
