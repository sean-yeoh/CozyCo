
<%= form_for :notice, url: notices_path  do |f| %>
  <p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </p>

  <p>
    <%= f.label :text %><br>
    <%= f.text_area :body %>
  </p>

  <p>
    <%= f.label :expiry_date %><br>
    <%= f.date_field :expiry_date %>
  </p>

  <p>
    <%= f.submit %>
  </p>

<% end %>
